{
	string		ids
	localized	name	

	[list Kingdom]
	{
		string	ids
	}
}

;; Specifies information about the dynamic campaign
[Template DynamicCampaignTemplate]
{
	;; General Properties
	string 		ids 
	localized 	name
	string		default_kingdom	
	localized	description
	int		initial_month
	int		initial_year

	;; Destroy behaviors
	;; - none - never destroy an army (unless there is no retreat available)
	;; - first - destroy first army into the territory
	;; - weakest - destroy the weakest army in terms of quickbattle odds
	;; - strongest - destroy the strongest army in terms of quickbattle odds
	;; - least_expensive - destroy the least expensive army in terms of total upgrade cost of all divisions in the army
	;; - most_expensive - destroy the most expensive army in terms of total upgrade cost of all divisions in the army
	;; - random - destroy 1 random army
	;; - all - destroy all armies
	string		attacking_destroy_behavior 	;; What army to destroy when you attack and lose
	string		defending_destroy_behavior	;; What army to destroy when you get attacked and lose

	;; Map
	string		map_ids
	string		map_table_ids

	;; VFX
	string		territory_friendly_vfx = territory_friendly_tint
	string		territory_enemy_vfx = territory_enemy_tint
	string		territory_friendly_arrow_vfx = territory_friendly_arrow
	string		territory_enemy_arrow_vfx = territory_enemy_arrow

	;; Cinematics (win cinematics specified with kingdom)
	string		cinematic_intro

	;; For the choose dynamic campaign interface
	string		choose_background_texture
	[list ChooseButtonVE]
	{
		string	when
		string	texture
	}

	[list Kingdom Template=DynamicCampaignKingdomTemplate]

	[list Team Template=DynamicCampaignTeamTemplate]

	[list Event Template=MGEventTemplate]
}

