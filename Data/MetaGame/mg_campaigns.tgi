;;-----------------------------------------------------------------------------
;; Templates for events that can happen in the dynamic campaign
;;-----------------------------------------------------------------------------

[Template MGEventTemplate]
{	
	string 		ids			;; IDS of the event
	localized	name			;; Name of the event

	;; The round when this event happens.
	;; If -1, then this event occurs randomly according to round_probability
	int 		round = -1

	;; The probability this event will happen in a particular round.  
	;; This should be set to something besides 1.0 if the round is not set.  
	;; If the round is set, this is the probability the event happens in that round
	;; (which should probably be 1.0).
	fixed		round_probability = 1.0

	;; The turn when this event happens.  If not specified and the event should happen
	;; that round, it will happen at a random turn.
	string 		kingdom_turn_ids = NULL

	;; Optional message to display when this event occurs.  If not specified 
	;; no message will be displayed.  If it includes %s, it will display the kingdom
	;; whose turn it happened on.
	localized	message = ""

	[List Effect]
}

[Template MGEventEffectTypeTemplate]
{

	;; The type of the event effect
	;; Possible types include:
	;; ADD_TECH
	;; REMOVE_TECH
	;; ADD_ACTOR
	;; REMOVE_ACTOR
	;; CHANGE_TERRITORY

	string		type

	;; Optional message to display when this effect occurs.  If not specified
	;; no message will be displayed.  The parameters will otherwise be inserted in 
	;; this priority order:
	;; %s - target kingdom
	;; %s - actor
	;; %s - territory
	;; %s - technology
	;; %s - source kingdom

	localized	message = ""	
}

[Template MGEventEffectTypeAddTechnologyTemplate Inherit=MGEventEffectTypeTemplate]
{
	string		type = ADD_TECH

	;; The kingdom to add the technology to.  
	;; If not specified, the target is the same as kingdom_turn_ids in the Event.
	string		kingdom_target_ids = NULL

	;; The technology to add.  If not specified, random technology
	string		tech_ids = NULL

	;; The message to display
	localized	message = "#mg_campaigns_MGEventEffectTypeAddTechnologyTemplate_message|ss"
}

[Template MGEventEffectTypeRemoveTechnologyTemplate Inherit=MGEventEffectTypeTemplate]
{
	string		type = REMOVE_TECH

	;; The kingdom to remove the technology from.  
	;; If not specified, the target is the same as kingdom_turn_ids in the Event.
	string		kingdom_target_ids = NULL

	;; The technology to remove.  If not specified, random technology
	string		tech_ids = NULL

	;; The message to display
	localized	message = "#mg_campaigns_MGEventEffectTypeRemoveTechnologyTemplate_message|ss"
}

[Template MGEventEffectTypeAddActorTemplate Inherit=MGEventEffectTypeTemplate]
{
	string		type = ADD_ACTOR

	;; The kingdom to add the actor to.
	;; If not specified, the target is the same as kingdom_turn_ids in the Event.
	string		kingdom_target_ids = NULL

	;; The territory to place the actor.
	;; If not specified, places the actor in a random territory.
	string		territory_ids = NULL

	;; The type of actor to add.  If not specified, random metagame actor.
	string		actor_ids = NULL

	;; The message to display
	localized	message = "#mg_campaigns_MGEventEffectTypeAddActorTemplate_message|sss"
}

[Template MGEventEffectTypeRemoveActorTemplate Inherit=MGEventEffectTypeTemplate]
{
	string		type = REMOVE_ACTOR

	;; The kingdom to remove the actor from.
	;; If not specified, the target is the same as kingdom_turn_ids in the Event.
	string		kingdom_target_ids = NULL

	;; This just chooses a random actor from the kingdom to remove 
	;; (Could be removed in any territory)

	;; The message to display
	localized	message = "#mg_campaigns_MGEventEffectTypeRemoveActorTemplate_message|sss"
}

[Template MGEventEffectTypeChangeTerritoryTemplate Inherit=MGEventEffectTypeTemplate]
{
	string		type = CHANGE_TERRITORY

	;; The kingdom from which to take a territory
	string		kingdom_source_ids = NULL

	;; The territory that will change ownership
	string		territory_ids = NULL

	;; The kingdom to which the territory will go.
	;; If not specified, the target is the same as kingdom_turn_ids in the Event.
	string		kingdom_target_ids = NULL

	;; Functionality:
	;; 1)  If the kingdom_source_ids is only specified, it takes a random territory from
	;;     that kingdom and transfers it to the target.
	;; 2)  If the territory_ids is only specified, it takes that particular territory from
	;;     whatever kingdom and transfers it to the target.
	;; 3)  If both are specified, it only transfers that particular territory if and only if 
	;;     it is owned by the specified kingdom.

	;; The message to display
	localized	message = "#mg_campaigns_MGEventEffectTypeChangeTerritoryTemplate_message|sss"
}

