[template EconomyComponent] 
{
	;; determines whether must be in supply for "Production" to apply
	;; HACK: default value here until in all the data
	flag	supply_required = false

	[Cost template=CostList]		;; positive numbers
	
	[Upkeep template=UpkeepList]	;; positive numbers
	
	[Production template=ProductionList]	;; positive numbers
}

