			[SpecificRecruitRequest]
			{
				property_ids		= infhq
			}

			[SpecificRecruitRequest]
			{
				property_ids		= airhq
			}

			[SpecificRecruitRequest]
			{
				property_ids		= supplydepot
			}
		}

		[ActorPriority]
		{
			property_IDS		= airhq
			value			= 8999
			repeat_penalty		= 100
		}

		[ActorPriority]
		{
			property_IDS		= infhq
			value			= 8800
			repeat_penalty		= 10000
		}

		[ActorPriority]
		{
			property_IDS		= special_company
			value			= 8997
			repeat_penalty		= 25
		}

		[ActorPriority]
		{
			property_IDS		= recon
			value			= 9000
			repeat_penalty		= 100
		}

		[ActorPriority]
		{
			property_IDS		= supplydepot
			value			= 1500
			repeat_penalty		= 100
