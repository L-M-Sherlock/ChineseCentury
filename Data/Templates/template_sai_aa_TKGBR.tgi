;; 第一阶段 (补HQ)
[template TK_AA_1_GBR template = TK_AA_PLAY_GBR]
{
	[Filters]
	{
		[Item]
		stat	= money_rate
		min	= -9999999
		max	= 120

		[Item]
		stat	= ammo_rate
		min	= 5
		max	= 99999

		[Item]
		stat	= oil_rate
		min	= 5
		max	= 99999
	}

	[GoalEngine]
	{
		build_queue_max_items	= 2

		[GoalPriority]
		{
			goal_IDS	= RESEARCH
			multiplier	= 0.1
			fuzziness	= 100
		}

		[Research template = TK1Research]

		[OffensiveCV]
		{
			cv_advantage_target		= 0.1
			cv_advantage_max		= 24
			cv_exponent_under_target	= 1
