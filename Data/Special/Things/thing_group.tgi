; Group thing
[Thing Template=Actor]
{
	IDS = group
	name = "#thing_group_name"
	category = GROUP

	[Flags]
	{
		blocking = false
		invisible = true
		ethereal = true
		invulnerable = true
	}

	;; TODO: remove once we get rid of height
	[Size]
	{
		height = 0
		blocking_shape = POINT
	}

	[View]
	{
		resource_prefix = NULL
	}
}
