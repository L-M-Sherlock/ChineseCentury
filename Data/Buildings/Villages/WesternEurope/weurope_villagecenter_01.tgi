/********************    CAMPAIGN MAP CENTERS    ********************/

[Thing Template=WesternEuropeVillageCenter]
{
	IDS = western_europe_city_center01_large
	editor_name = "#western_europe_city_center01_large_editor_name"

	[EconomyComponent Template=EconomyComponentCityLarge]

	[CityComponent]
	{	
		[Layout Template=CityLayout]
		{
			[Object Template=CityObject]
			object_ids = temperate_trees
			offset = -6,-6
			orientation = 0, -1
		}
	} 
}

